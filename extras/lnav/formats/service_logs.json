{
    "$schema": "https://lnav.org/schemas/format-v1.schema.json",
    "plain_log_format": {
        "title": "Custom Plain Text Log Format for Service Logs",
        "description": "Format for parsing service logs in 'service-name | message' format",
        "regex": {
            "basic": {
                "pattern": "^(?<service>[^|]+) \\| (?<message>.+)$"
            }
        },
        "value": {
            "service": {
                "kind": "string",
                "identifier": true
            },
            "message": {
                "kind": "string",
                "identifier": false
            },
            "message_start": {
                "kind": "string",
                "hidden": true,
                "identifier": false
            }
        },
        "sample": [
            {
            "line": "frontend-external | Error connecting to database."
            }
        ]
    }
}